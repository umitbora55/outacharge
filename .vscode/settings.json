{
    "css.lint.unknownAtRules": "ignore",
    "postcss.lint.unknownAtRules": "ignore",
    "scss.lint.unknownAtRules": "ignore",
    "css.customData": [
        ".vscode/tailwind-v4.json"
    ],
    "files.associations": {
        "*.css": "postcss"
    },
    "tailwindCSS.experimental.classRegex": [
        [
            "@custom-variant\\s+([^\\s]+)\\s+",
            "([^;]+)"
        ]
    ]
}